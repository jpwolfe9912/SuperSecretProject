  35:Core/Src/esp32.c **** bool Wifi_WaitForString(uint32_t TimeOut_ms, uint8_t *result, uint8_t CountOfParameter, ...)
  36:Core/Src/esp32.c **** {
  37:Core/Src/esp32.c ****     /*
  38:Core/Src/esp32.c ****      * It uses the CountOfParameter and the Parameters after that to compare with the
  39:Core/Src/esp32.c ****      * information that it was received in the UART RX. If the parameter is in the
  40:Core/Src/esp32.c ****      * received string the functions return a true value and the position of the
  41:Core/Src/esp32.c ****      * parameter (according to the position in the function)
  42:Core/Src/esp32.c ****      *
  43:Core/Src/esp32.c ****      * Ex:
  44:Core/Src/esp32.c ****      * Wifi_WaitForString(_WIFI_WAIT_TIME_LOW,&result,2,"OK","ERROR")
  45:Core/Src/esp32.c ****      *
  46:Core/Src/esp32.c ****      * If the ESP8266 return a AT+OK after the last command, the function is going to
  47:Core/Src/esp32.c ****      * return a true value and the result number would be 1.
  48:Core/Src/esp32.c ****      */
  49:Core/Src/esp32.c **** 
  50:Core/Src/esp32.c ****     if (result == NULL)
  51:Core/Src/esp32.c ****         return false;
  52:Core/Src/esp32.c ****     if (CountOfParameter == 0)
  53:Core/Src/esp32.c ****         return false;
  54:Core/Src/esp32.c **** 
  55:Core/Src/esp32.c ****     *result = 0;
  56:Core/Src/esp32.c **** 
  57:Core/Src/esp32.c ****     va_list tag;
  58:Core/Src/esp32.c ****     va_start(tag, CountOfParameter);
  59:Core/Src/esp32.c ****     char *arg[CountOfParameter];
  60:Core/Src/esp32.c ****     for (uint8_t i = 0; i < CountOfParameter; i++)
  61:Core/Src/esp32.c ****         arg[i] = va_arg(tag, char *);
  62:Core/Src/esp32.c ****     va_end(tag);
  63:Core/Src/esp32.c **** 
  64:Core/Src/esp32.c ****     for (uint32_t t = 0; t < TimeOut_ms; t += 20)
  65:Core/Src/esp32.c ****     {
  66:Core/Src/esp32.c ****         delayMicroseconds(20000);
  67:Core/Src/esp32.c ****         for (uint8_t mx = 0; mx < CountOfParameter; mx++)
  68:Core/Src/esp32.c ****         {
  69:Core/Src/esp32.c ****             if (strstr((char *)Buffs.RxBuffer.buff, arg[mx]) != NULL)  // TODO change to lwrb_find(
  70:Core/Src/esp32.c ****             // if (strstr((char *)Buffs.RxBuffer.buff, arg[mx]) != NULL)
  71:Core/Src/esp32.c ****             {
  72:Core/Src/esp32.c ****                 *result = mx + 1;
  73:Core/Src/esp32.c ****                 return true;
  74:Core/Src/esp32.c ****             }
  75:Core/Src/esp32.c ****         }
  76:Core/Src/esp32.c ****     }
  77:Core/Src/esp32.c ****     // timeout
  78:Core/Src/esp32.c ****     return false;
  79:Core/Src/esp32.c **** }
